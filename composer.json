{
    "name": "hanaboso/pipes-php-sdk",
    "description": "PHP sdk for PipesFramework",
    "version": "1.6.2",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-mongodb": "*",
        "ext-oauth": "*",
        "doctrine/mongodb-odm-bundle": "^4.3",
        "hanaboso/commons-bundle": "^2.3",
        "hanaboso/mongo-data-grid": "^1.5",
        "hanaboso/rabbit-mq-bundle": "^1.5",
        "league/oauth2-client": "^2.6",
        "phpoffice/phpspreadsheet": "^1.18",
        "symfony/monolog-bundle": "^v3.7",
        "symfony/yaml": "^v5.3"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.2",
        "hanaboso/rest-bundle": "^1.1",
        "symfony/debug-bundle": "^v5.3",
        "symfony/security-bundle": "^v5.3",
        "symfony/serializer": "^v5.3"
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\PipesPhpSdk\\": "src/"
